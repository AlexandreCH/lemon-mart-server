{
  "compilerOptions": {
    "target": "es2021",
    "module": "Node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": false, // enabling breaks js file hiding in IDE
    "lib": ["es2023"],
    "typeRoots": ["node_modules/@types"],
    "skipLibCheck": true,
    "resolveJsonModule": true
  }
}
